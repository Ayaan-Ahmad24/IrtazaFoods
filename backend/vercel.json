{
    "installCommand":"npm install --legacy-peer-deps",
    "version": 2,
    "builds": [
      {
        "src": "app.js",
        "use": "@vercel/node"
      },
      {
        "src":"src/**/*",
        "builds":"@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
  }
  